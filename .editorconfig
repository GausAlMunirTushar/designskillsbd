# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true
[*]
indent_style = tab
indent_size = 4
tab_width = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.js]
max_line_length = 80

[*.md]
max_line_length = off
trim_trailing_whitespace = false
